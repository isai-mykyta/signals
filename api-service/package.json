    "typescript": "^5.7.2",
    "umzug": "^3.8.2"
  },
  "scripts": {
    "start:dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "start:prod": "tsc && node dist/server.js",
    "test": "jest --verbose --silent",
    "db:up": "docker-compose --env-file .env up -d",
    "db:down": "docker-compose down",
    "db:restart": "npm run db:down && npm run db:up",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migration:create": "npx sequelize-cli migration:generate --name"
